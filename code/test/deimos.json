{
   "section" : {
      "order" : "1",
      "re" : "^\\s*-*%+\\s*(.*?)\\s*%+"
   },
   "author" : {
      "order" : "1.1",
      "re" : "^\\s*[Bb]y\\s+(.*)",
      "attrs" : {
         "author_attr" : {
            "re" : "\\s+\\(([^\\(\\)]*)\\)\\s*$"
         }
      }
   },
   "series" : {
      "order" : "1.1.1",
      "re" : "^\\s*=+\\/?\\s*(.*?)\\s*\\/?=+"
   },
   "title" : {
      "order" : "1.1.1.1",
      "re" : "^\\s*>\\s*(.*)",
      "attrs" : {
         "title_attr" : {
            "re" : "\\s+\\(([^\\(\\)]*?)\\)\\s*$"
         }
      }
   },
   "tags" : {
      "order" : "1.1.1.1.1",
      "re" : "^\\s*(\\[.*)",
      "attrs" : {
         "anthro" : {
            "re" : "(?x) ^\\[  ([^\\[\\]]*)",
            "order" : 1,
            "delims" : [ ";", "," ]
         },
         "general" : {
            "re" : "(?x) \\]\\[ ([^\\[\\]]*) \\]",
            "order" : 2,
            "delims" : [ ";", "," ]
         },
         "ops" : {
            "re" : "(?x) ([^\\[\\]]+) $",
            "order" : 3,
            "delims" : [""]
         }
      }
   },
   "url" : {
      "order" : "1.1.1.1.2",
      "re" : "^\\s*(http.+)",
      "attrs" : {
         "url_attr" : {
            "re" : "\\s*\\((.*)\\)"
         }
      }
   },
   "description" : {
      "order" : "1.1.1.1.3",
      "re" : "^\\s*#(.*)"
   }
}
